{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "Game of tic tac toe using html, css, and javascript",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "eslint": "eslint **/*.js",
    "prettier": "prettier **/*.js **/*.html **/*.json",
    "lint-staged": "npm run eslint && npm run prettier",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callmenick/tic-tac-toe.git"
  },
  "author": "Nick Salloum",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/callmenick/tic-tac-toe/issues"
  },
  "homepage": "https://github.com/callmenick/tic-tac-toe#readme",
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/preset-env": "7.20.2",
    "babel-jest": "29.3.1",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.7",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1"
  },
  "lint-staged": {
    "**/*.{js}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "**/*.{json,html}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
